<% if(success != "") { %>
<div class="alert alert-success alert-dismissible" role="alert">
  <%=success %>
  <button type="button" class="btn-close btn-sm" aria-label="Close" data-bs-dismiss="alert"></button>
</div>
<% } %>

<% if(error != "") { %>
<div class="alert alert-danger alert-dismissible" role="alert">
  <%=error %>
  <button type="button" class="btn-close btn-sm" aria-label="Close" data-bs-dismiss="alert"></button>
</div>
<% } %>
